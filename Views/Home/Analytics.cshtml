                                        <input type="radio" value="1" asp-for="ROADWAY_DEPARTURE" class="col-2 mt-sm-3" />
                                        <div class="col-9 btn-warning text-center">
                                            Roadway Departure
                                            <div class="text-secondary font-weight-bold small">@ViewData["Road_Departure"] of total crashes</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-12 mb-4">
                                <div class="card text-white border-0">
                                    <div class="row">
                                        <input type="radio" value="1" asp-for="DISTRACTED_DRIVING" class="col-2 mt-sm-3" />
                                        <div class="col-9 btn-warning text-center">
                                            Distrcted Driving
                                            <div class="text-secondary font-weight-bold small">@ViewData["Distracted_Driving"] of total crashes</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-12 mb-4">
