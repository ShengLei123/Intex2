                                            Wild Animal
                                            <div class="text-secondary font-weight-bold small">@ViewData["Wild_Animal"] of total crashes</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-12 mb-4">
                                <div class="card text-white border-0">
                                    <div class="row">
                                        <input type="radio" value="1" asp-for="DROWSY_DRIVING" class="col-2 mt-sm-3" />
                                        <div class="col-9 btn-warning text-center">
                                            Drowsy Driving
                                            <div class="text-secondary font-weight-bold small">@ViewData["Drowsy_Driving"] of total crashes</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                        </div>
                        <div class="col-lg-12 mt-4">
                            <button type="button" class="btn btn-block btn-success" id="clear_tag_radio">Clear Selections</button>
                        </div>
                        @*<div class="row">
                            <div class="col-lg-6 mb-4">
                                <div class="card bg-info text-white shadow">
                                    <div class="card-body">
                                        Info
                                        <div class="text-white-50 small">#36b9cc</div>
