                                    <input type="radio" value="@ViewBag.Min_Date_List[i]" asp-for="CRASH_DATETIME" class="col-sm-1 mt-3" />
                                    <div class="col-sm-10 justify-content-end">
                                        <h4 class="small font-weight-bold">
                                            @ViewBag.Unique_Year[i]<span class="float-right">@ViewBag.Min_Date_List_Percent[i]</span>
                                        </h4>
                                        <div class="progress mb-4">
                                            <div class="progress-bar bg-danger" role="progressbar" style="width: @ViewBag.Min_Date_List_Percent[i]"
                                                 aria-valuenow="@ViewBag.Min_Date_List_Int[i]" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                    </div>
                                    <br />
                                </div>
                            }
                        </div>
                        <div class="col-lg-12 mt-5">
                            <button type="button" class="btn btn-block btn-success" id="clear_year_radio">Clear Selections</button>
                        </div>

                    </div>
                
                </div>

