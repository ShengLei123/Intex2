                                            @ViewBag.Unique_County[x] County
                                            <div class="text-white-50 font-weight-bold small">@ViewBag.Unque_County_Percent[x] of total crashes</div>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                        
                        <div class="col-lg-12 mt-4">
                            <button type="button" class="btn btn-block btn-success" id="clear_county_radio">Clear Selections</button>
