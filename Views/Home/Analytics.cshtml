                                <h4 class="small font-weight-bold">
                                    Crash Severity ID 1<span class="float-right">@ViewData["Severity_1_Percent"]</span>
                                </h4>

                                <div class="progress mb-4">
                                    <div class="progress-bar bg-danger" role="progressbar" style="width: @ViewData["Severity_1_Percent"]"
                                         aria-valuenow="@ViewData["Severity_1_Int"]" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>
                            </label>
                            <br />
                        </div>
                        <div class="row">
                            <input type="radio" value="2" asp-for="CRASH_SEVERITY_ID" class="mt-3" />
                            <div class="col-sm-11 justify-content-end">
